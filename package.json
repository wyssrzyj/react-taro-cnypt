{
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "description": "优产移动端",
  "templateInfo": {
    "name": "mobx",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp:prod": "cross-env NODE_ENV=production taro build --type weapp --watch",
    "build:weapp:test": "cross-env NODE_ENV=test taro build --type weapp --watch",
    "build:weapp:dev": "cross-env NODE_ENV=development taro build --type weapp ",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp:dev -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/runtime": "3.2.6",
    "@tarojs/taro": "3.2.6",
    "@tarojs/components": "3.2.6",
    "mobx": "^6.1.8",
    "mobx-react": "^7.1.0",
    "react-dom": "^17.0.0",
    "@tarojs/react": "3.2.6",
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.2.6",
    "@tarojs/webpack-runner": "3.2.6",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "ali-oss": "^6.16.0",
    "babel-preset-taro": "3.2.6",
